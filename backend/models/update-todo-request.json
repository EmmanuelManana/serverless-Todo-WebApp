{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "todo",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",      
      "minLength": 3,
      "maxLength": 20
    },
    "dueDate": {
      "type": "string",
      "pattern":"^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
    },
    "done": {
      "done": "boolean"
    }
  },
  "required": ["name","dueDate", "done"]
}

